{
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "plugins": [
    "jsdoc-plugin-typescript",
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "typescript": {
    "moduleRoot": "src"
  }
}
